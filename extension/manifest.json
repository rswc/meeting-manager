{
    "description": "hey guys look at my cool addon",
    "manifest_version": 2,
    "name": "Meeting manager",
    "version": "1.0",
    "icons": {
      "48": "icons/48.png"
    },
  
    "browser_specific_settings": {
      "gecko": {
        "id": "meeting_manager@bigblueboys.hacknarok",
        "strict_min_version": "50.0"
      }
    },
  
    "background": {
      "scripts": ["background.js"]
    },
  
    "browser_action": {
      "default_icon": "icons/48.png",
      "default_title": "Meeting manager"
    },
  
    "permissions": [
        "webRequest",
        "nativeMessaging",
        "*://*.zoom.us/*",
        "*://*.mozilla.org/*"
    ]
  }